      <ListView fx:id="saleListView" layoutX="15.0" layoutY="15.0" prefHeight="320.0" prefWidth="140.0" />
      <Button onAction="#dodajSale" layoutX="170.0" layoutY="15.0" mnemonicParsing="false" prefWidth="85.0" text="Dodaj salę" />
      <Button onAction="#edytujSale" layoutX="170.0" layoutY="55.0" mnemonicParsing="false" prefWidth="85.0" text="Edytuj salę" />
      <Button onAction="#usunSale" layoutX="170.0" layoutY="95.0" mnemonicParsing="false" prefWidth="85.0" text="Usuń salę" />
   </children>
</AnchorPane>
